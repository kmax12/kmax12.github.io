    <div id="screenshot-section" class="container section">
		<div class="row">
			<div class="col s12">
				<div class="section-header center-align">Stay in touch</div>
			</div>
			<div class="col s12 center-align">
				<a href="http://eepurl.com/bC4eaX" class="waves-effect waves-light btn green-bg">Sign up for newsletter</a>
			</div>
		</div>
