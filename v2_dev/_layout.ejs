<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title></title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="shortcut icon" href="/img/favicon.ico" type="image/x-icon" />

    <link href='http://fonts.googleapis.com/css?family=Roboto:400,700,500,300' rel='stylesheet' type='text/css'>
      
    <!-- Compiled and minified CSS -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.0/css/materialize.min.css">

    <!-- Compiled and minified JavaScript -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.0/js/materialize.min.js"></script>


    <link rel="stylesheet" href="/css/main.css">
    <link rel="stylesheet" href="/v2/css/main.css"> <!-- hack to to handle hosting at /v2 -->
  </head>
  <body>
    <%- partial("_navbar") %>
    
    <!-- If the current page is industry/ but not blog/index.ejsâ€¦ -->
    <% if(current.path[0] == "industry" && current.source !== 'index') { %>
      <!-- Render the partial blog/_nest -->
      <%- partial(current.path[0] + "/_nest") %>
    <% } else { %>
    <!-- Otherwise, render the yield -->
        <%- yield %>
    <% } %>

    <div class="bg">
      <div id="footer" class="container section">
        <!-- Cloud
      Run FeatureLab in a secure cloud
       -->
      <div class="row">
        <div class="col s4">
          <p><a href="/">Home</a></p>
          <p><a href="/#why">Why FeatureLab?</a></p>
          <p><a href="/#features">Product Features</a></p>
          </div>
          <div class="col s4">
            <p><a href="/industries">Industries</a></p>
            <% for(var i in public.industry._data){ %>
              <p><a href="/industry/<%- i %>"><%- public.industry._data[i].name %></a></p>
            <% }; %>            
          </div>
          <div class="col s4">
          </div>
      </div>

      
        
      </div>
  </body>
</html>